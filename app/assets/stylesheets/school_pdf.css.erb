}

.mgt220 {
    margin-top: 0px;
}

.pd0 {
    padding: 0;
    margin: 0;
}

ul {
    font-size: 1.1rem;
    padding: 10px 20px 0;
    margin-bottom: 0;
}

li {
    padding-bottom: 10px;
    line-height: 1.4em;
}

li:last-child {
    padding-bottom: 0;
}

ul li ul li {
    list-style: square;
}

ul li ul li {
    padding-bottom: 0px;
}


/*********************************************************************


                                FOOTER


**********************************************************************/
.footer,
.obs_page {
    position: relative;
}

.obs {
    position: absolute;
    bottom: 0px;
    margin-left: 60px;
}

.obs_logo {
    position: absolute;
    bottom: 0px;
    right: 0px;
    margin-right: 60px;
    margin-bottom: 60px;
}

#footer {
    background: #eee;
    padding: 10px;
    font-size: 12px;
    color: #999;
    width: 100%;
    position: absolute;
    bottom: 0px
}

#footer a {
    color: #ccc;
}

#footer a:hover {
    color: #fff;
}

#footer h3.widget-title {
    font-size: 15px;
    font-size: 0.9375rem;
    text-transform: uppercase;
    color: #999;
    margin: 0;
}

