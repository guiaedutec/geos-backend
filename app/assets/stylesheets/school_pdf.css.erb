html {
    font-size: 100%;
}

body {
    font-size: 14px;
    font-family: "DejaVu Sans", Helvetica, Arial, sans-serif;
    color: #222222;
    width: 100%;
    margin: 0;
    padding: 0;
}

.topspace {
    margin-top: 40px;
}

a {
    text-decoration: none !important;
}

/*********************************************************************


                                TYPOGRAPHY


**********************************************************************/
p {
    line-height: 1.4em;
    margin: 0 0 20px 0;
}

ul,
ol {
    line-height: 1.4em;
    margin: 0 0 20px 0;
}

blockquote {
    padding: 10px 20px;
    margin: 0 0 30px;
    border-left: 5px solid #eee;
}

h1,
h2,
h3,
h4,
h5,
h6,
.h1,
.h2,
.h3,
.h4,
.h5,
.h6 {
    font-family: "DejaVu Sans", Helvetica, Arial, sans-serif;
    font-weight: 400;
    color: #000000;
}

h1 {
    font-size: 40px;
    font-size: 2.5rem;
}

h2 {
    font-size: 36px;
    font-size: 2.25rem;
}

h3 {
    font-size: 30px;
    font-size: 1.875rem;
}

h4 {
    font-size: 24px;
    font-size: 1.2rem;
}

h5 {
    font-size: 20px;
    font-size: 1.25rem;
}

a {
    color: #141e3b;
}

a:hover {
    color: #e93675;
}

.lead {
    font-weight: 300;
    font-size: 1.425rem;
}

/*********************************************************************


                                HEADER


**********************************************************************/
#head img.img-circle {
    display: block;
    width: 140px;
    height: 140px;
    overflow: hidden;
    border: 9px solid rgba(0, 0, 0, 0.05);
    margin: 0 auto;
}

#head .title {
    font-size: 99px;
    color: #fff;

}

#head .title a {
    text-decoration: none;
    color: #fff;
}

#head .tagline {
    display: block;
    font-size: 16px;
    line-height: 1.2em;
    color: #fff;
    margin: 5px 0 0;
}

#head .tagline b {
    font-weight: normal;
}

#head .tagline a {
    color: #5E5E5E;
}

.home #head {
    padding: 0;
    background-size: auto;
    height: 990px;
}

.home #head .title {
    font-size: 60px;
    font-size: 3.2rem;
    color: #fff;
    text-transform: uppercase;
}

.home #head .tagline {
    font-size: 18px;
    font-style: italic;
    margin: 30px 0;
    width: 500px;
}

.head_spacer {
    height: 300px;
}

.mgb40 {
    margin-bottom: 40px;
}

.mgt0 {
    margin-top: 0;
}

.divisor {
    background: #fff;
    margin: 40px 0;
    height: 20px;
    width: 140px;
}

/*********************************************************************


                                CONTENT


**********************************************************************/
img {
    max-width: 100%;
}

.widget img {
    height: 90px;
    margin-right: 25px;
}

.widget {
    text-align: center;
}

.inline {
    display: inline-block;
}

p.detail_a {
    margin-top: 240px;
    font-size: 14px;
    color: #141e3b;
}

p.detail_a_pdf {
    margin-top: 150px;
    font-size: 10px;
    color: #141e3b;
}

p.resp-by {
    margin-top: 50px;
