            .col-xs-12
              %p.lead.text-muted
                #{fb.body["0"]}
              %p.lead.text-muted
                #{fb.body["1"]}
                - if @user._profile.to_s == "principal" || @user._profile.to_s == "teacher"
                  - if @school.type == "Estadual"
                    Estado #{@school.state_name}
                  - if @school.type == "Municipal"
                    Município de #{@school.city_name}
                - else
                  [ESTADO|CIDADE],
                #{fb.body["2"]}
              %p.lead.text-muted
                #{fb.body["3"]}
      %footer#footer
        .container
          .row
            .col-xs-12e.widget
              - if @school
                - if !@feedbackImages.nil?
                  - images = @feedbackImages
                  - images.each do |imageUpload|
                    .inline
                    =image_tag("#{imageUpload.file_upload.url}",{width: 90, height: 90})
              .inline
                =wicked_pdf_image_tag('devolutive/imgLogoHeader.png', :width => "260")
