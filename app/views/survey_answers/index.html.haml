- model_class = SurveyAnswer
.page-header
  %h1=t '.title', :default => model_class.model_name.human.pluralize.titleize
.col-xs-12
  .row= paginate @survey_answers

%table.table.table-striped
  %thead
    %tr
      %th= model_class.human_attribute_name(:id)
      %th= model_class.human_attribute_name(:value)
      %th= model_class.human_attribute_name(:position)
      %th= model_class.human_attribute_name(:survey)
      %th= model_class.human_attribute_name(:survey_answer_id)
      %th= model_class.human_attribute_name(:survey_question_id)
      %th= model_class.human_attribute_name(:created_at)
      %th=t '.actions', :default => t("helpers.actions")
  %tbody
    - @survey_answers.each do |survey_answer|
      %tr
        %td= link_to survey_answer.id, survey_answer_path(survey_answer)
        %td= survey_answer.value
        %td= survey_answer.position
        %td= survey_answer.survey
        %td= survey_answer.survey_answer_id
        %td= survey_answer.survey_question_id
        %td=l survey_answer.created_at
        %td
          = link_to t('.edit', :default => t("helpers.links.edit")), edit_survey_answer_path(survey_answer), :class => 'btn btn-default btn-xs'
          = link_to t('.destroy', :default => t("helpers.links.destroy")), survey_answer_path(survey_answer), :method => :delete, :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-xs btn-danger'

= link_to t('.new', :default => t("helpers.links.new")), new_survey_answer_path, :class => 'btn btn-primary'
.col-xs-12
  .row= paginate @survey_answers
